{% extends base %}

{% block body %}
<div style="border:2px solid black;width:100%">
    {% for row1 in rows %}
    <div style="display:flex;">
        {% for i1 in row1 %}
        <div style="border:1px solid black;width:100%">
            {% for row2 in i1 %}
            <div style="display:flex;">
                {% for i2 in row2 %}
                <div style="width:100%;height:40px;display: flex;justify-content: center;align-items: center;border:1px solid black">
                    <h2>{{ i2 }}</h2>
                </div>
                {% endfor %}
            </div>
            {% endfor %}
        </div>
        {% endfor %}
    </div>
    {% endfor %}
</div>
{% endblock body %}